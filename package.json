{
  "name": "abdolian-test-02",
  "version": "2.3.4",
  "license": "MIT",
  "sideEffects": false,
  "author": "Masood Abdolian <m.abdolian@gmail.com>",
  "description": "A powerful library for building scalable, reusable, fast, tastable and lightweight design system for any web technologies. Powerd by Web Component.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/elements/index.js",
      "types": "./dist/types/elements/index.d.ts"
    },
    "./*": {
      "import": "./dist/elements/*",
      "types": "./dist/types/elements/*/*.d.ts"
    },
    "./*/*": {
      "import": "./dist/elements/*/*",
      "types": null
    },
    "./config.js": {
      "import": "./dist/core/config.js",
      "types": "./dist/types/config/index.d.ts"
    },
    "./json/*.json": {
      "import": "./dist/json/*.json",
      "types": null
    },
    "./package.json": {
      "import": "./package.json",
      "types": null
    }
  },
  "files": [
    "./dist/*",
    "./package.json",
    "./README.md"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rimraf dist",
    "development:start": "vite --config development/vite.config.js",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">= 20.10.0"
  },
  "keywords": [
    "components",
    "custom elements",
    "design systems",
    "web components"
  ],
  "dependencies": {
    "@babel/generator": "^7.24.10",
    "@babel/parser": "^7.24.8",
    "@babel/template": "^7.24.7",
    "@babel/traverse": "^7.24.8",
    "@babel/types": "^7.24.9",
    "@htmlplus/ui": "^1.4.1",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "change-case": "^5.4.4",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "ora": "^8.0.1",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "cpy": "^11.0.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.20.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-node-externals": "^7.1.2",
    "semantic-release": "^24.0.0",
    "tslib": "^2.6.3",
    "vite": "^5.3.4"
  }
}
